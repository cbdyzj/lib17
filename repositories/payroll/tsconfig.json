{
  "compilerOptions": {
    "module": "ES2020",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "target": "ES2020",
    "outDir": "dist",
    "jsx": "react",
    "allowJs": false,
    "lib": [
      "ES2020",
      "DOM"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "static/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "static"
  ]
}
